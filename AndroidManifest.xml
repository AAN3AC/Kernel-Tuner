<activity android:name=".check" android:label="@string/title_check" android:theme="@style/Theme.TransparentNoTitle" android:icon="@drawable/icon"></activity>
<activity android:name=".profiles" android:label="@string/title_profiles" android:icon="@drawable/ic_menu_cc"></activity>
<activity android:name=".ProfileEditor" android:label="@string/activity_title_profile_editor" android:icon="@drawable/ic_menu_edit" android:theme="@style/Theme.Transparent"></activity>
<activity android:name=".Swap" android:label="@string/swap" android:icon="@drawable/swap" ></activity>

<activity android:name=".CPUTuner" android:label="@string/title_cpu" android:icon="@drawable/icon" android:screenOrientation="portrait" ></activity>
<activity android:name=".SystemInfo" android:label="@string/info" android:icon="@drawable/info" ></activity>


<service android:name=".WidgetUpdateService"></service>
<service android:name=".WidgetUpdateServiceBig"></service>

  <activity android:name="com.google.ads.AdActivity"
