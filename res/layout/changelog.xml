        android:text="-[FIX]Fixed bugs for quad-core devices\n-[ADD]Landscape mode\n-[FIX]Widget improvements"
 />
    
    <RelativeLayout
        android:id="@+id/rl25"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
