    </RelativeLayout>

    <TextView
        android:id="@+id/textView50"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rl25"
        android:text="-[ADD]Auto update-check every 24h\n-[ADD]Click notificaton to install update after download"
 />
    
</RelativeLayout>
</ScrollView>