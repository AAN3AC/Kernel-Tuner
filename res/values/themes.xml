        <item name="android:listChoiceIndicatorSingle">@drawable/btn_radio_holo_light_miui</item>
		<item name="android:spinnerStyle">@style/Miui.Spinner.Light</item>
		<item name="android:editTextStyle">@style/Miui.EditText.Light</item>
		<item name="android:track">@drawable/switch_track_holo_light_miui</item>
        <item name="android:thumb">@drawable/switch_inner_holo_light_miui</item>
        <item name="android:textOn">O</item>
        <item name="android:textOff">I</item>
        <item name="android:thumbTextPadding">12dip</item>
        <item name="android:switchMinWidth">64dip</item>
        <item name="android:switchPadding">16dip</item>
    </style>
	
